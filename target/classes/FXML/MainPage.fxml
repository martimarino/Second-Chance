<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<AnchorPane fx:id="anchorRoot" style="-fx-background-color: rgb(230, 230, 255);"  prefHeight="800.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.it.unipi.dii.largescale.secondchance.controller.MainController">
   <children>
      <TabPane fx:id ="tabpane" prefHeight="800.0" prefWidth="700.0" tabClosingPolicy="UNAVAILABLE" style="-fx-background-color: white; ">
        <tabs>
            <Tab text="Home">
                <content>
                  <fx:include fx:id="home" source="Home.fxml" />
                </content>
            </Tab>
            <Tab text="MyProfile">
                <content>
                    <fx:include fx:id="Profile" source="Profile.fxml" />
                </content>
            </Tab>
            <Tab text="My Orders">
                <content>
                    <fx:include fx:id="MyOrders" source="MyOrders.fxml" />
                </content>
            </Tab>
            <Tab text="Search User">
                <content>
                    <fx:include fx:id="SearchUser" source="SearchUser.fxml" />
                </content>
            </Tab>
            <Tab text="Search Insertion">
                <content>
                    <fx:include fx:id="SearchInsertion" source="SearchInsertion.fxml" />
                </content>
            </Tab>
            <Tab text="New insertion">
                <content>
                    <fx:include fx:id="NewInsertion" source="NewInsertion.fxml" />
                </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
