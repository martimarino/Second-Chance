<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane fx:id="anchorRoot" prefHeight="800.0" prefWidth="700.0" style="-fx-background-color: rgb(230, 230, 255);" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.HomeController">
   <children>
      <Text fill="#2582bf" layoutX="250.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="View viral insertions">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <BorderPane fx:id="viralInsertions" layoutX="25.0" layoutY="70.0" prefHeight="286.0" prefWidth="650.0">
         <left>
            <Button fx:id="prevButton" mnemonicParsing="false" onMouseClicked="#prevViralInsertions" prefHeight="45.0" prefWidth="45.0" style="-fx-background-image:url('../img/prev.png'); -fx-background-size: cover; -fx-background-repeat: no-repeat; -fx-background-position: center;" text="prev" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Button fx:id="nextButton" mnemonicParsing="false" onMouseClicked="#nextViralInsertions" prefHeight="45.0" prefWidth="45.0" text="next" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>

      <Text fill="#2582bf" text="Feed" layoutX="320.0" layoutY="430.0" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <BorderPane fx:id="feed" layoutX="25.0" layoutY="440.0" prefHeight="286.0" prefWidth="650.0">
         <left>
            <Button fx:id="prevFeedButton" mnemonicParsing="false" onMouseClicked="#prevFeedInsertions" prefHeight="45.0" prefWidth="45.0" text="prev" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Button fx:id="nextFeedButton" mnemonicParsing="false" onMouseClicked="#nextFeedInsertions" prefHeight="45.0" prefWidth="45.0" text="next" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </children>
</AnchorPane>
