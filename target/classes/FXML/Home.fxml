<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<AnchorPane fx:id="anchorRoot" prefHeight="800.0" prefWidth="700.0" style="-fx-background-color: rgb(70,130,180);" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.it.unipi.dii.largescale.secondchance.connection.controller.HomeController">
   <children>
      <Text fill="WHITE" layoutX="250.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="View viral insertions">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <BorderPane fx:id="viralInsertions" layoutX="25.0" layoutY="70.0" prefHeight="286.0" prefWidth="650.0">
         <left>
            <Pane fx:id="prevViralButton" onMouseClicked="#prevViralInsertions" translateY="100.0">
               <children>
                  <ImageView fitHeight="50.0" fitWidth="50.0" BorderPane.alignment="CENTER">
                     <image>
                        <Image url="@../img/prev.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
         </left>
         <right>
            <Pane fx:id="nextViralButton" onMouseClicked="#nextViralInsertions" translateY="100.0">
               <children>
                  <ImageView fitHeight="50.0" fitWidth="50.0" BorderPane.alignment="CENTER">
                     <image>
                        <Image url="@../img/next.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
         </right>
      </BorderPane>

      <Text fill="WHITE" layoutX="320.0" layoutY="430.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Feed">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <BorderPane fx:id="feed" layoutX="25.0" layoutY="440.0" prefHeight="286.0" prefWidth="650.0">
         <left>
            <Pane fx:id="prevFeedButton" onMouseClicked="#prevFeedInsertions" translateY="100.0">
               <children>
                  <ImageView fitHeight="50.0" fitWidth="50.0" BorderPane.alignment="CENTER">
                     <image>
                        <Image url="@../img/prev.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
         </left>
         <right>
            <Pane fx:id="nextFeedButton" onMouseClicked="#nextFeedInsertions" translateY="100.0">
               <children>
                  <ImageView fitHeight="50.0" fitWidth="50.0" BorderPane.alignment="CENTER">
                     <image>
                        <Image url="@../img/next.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
         </right>
      </BorderPane>
   </children>
</AnchorPane>
