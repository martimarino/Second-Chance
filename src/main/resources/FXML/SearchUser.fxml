<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorRoot" prefHeight="820.0" prefWidth="680.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.SearchUserController">
    <children>
        <Text layoutX="297.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Find Users">
            <font>
                <Font size="24.0" />
            </font>
        </Text>
        <ComboBox fx:id="country" layoutX="35.0" layoutY="130.0" prefHeight="25.0" prefWidth="105.0" value="country">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Italy" />
                    <String fx:value="Netherlands" />
                    <String fx:value="Spain" />
                    <String fx:value="Austria" />
                    <String fx:value="Poland" />
                    <String fx:value="Ireland" />
                    <String fx:value="France" />
                    <String fx:value="Canada" />
                    <String fx:value="United Kingdom (Great Britain)" />
                    <String fx:value="Brazil" />
                </FXCollections>
            </items>
        </ComboBox>
        <ComboBox fx:id="rating" layoutX="140.0" layoutY="130.0" prefWidth="100.0" value="rating">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="1" />
                    <String fx:value="2" />
                    <String fx:value="3" />
                    <String fx:value="4" />
                    <String fx:value="5" />
                </FXCollections>
            </items>
        </ComboBox>
        <TextField fx:id="us" layoutX="28.0" layoutY="81.0" prefWidth="560" promptText="Search users" />
        <Button fx:id="findUsers" alignment="CENTER" contentDisplay="RIGHT" layoutX="597.0" layoutY="80.0" mnemonicParsing="false" onMouseClicked="#findUsers" prefHeight="20.0" prefWidth="70.0" style="-fx-background-color: none; -fx-border-color: black; -fx-border-radius: 5;" text="Search" />
        <BorderPane fx:id="userFind" layoutX="249.0" layoutY="486.0" prefHeight="307.0" prefWidth="205.0">
            <left>
                <Button fx:id="prevButton" layoutX="-20.0" layoutY="-20.0" mnemonicParsing="false" onMouseClicked="#showPrevUser" text="Prev" />
            </left>
            <right>
                <Button fx:id="nextButton" mnemonicParsing="false" onMouseClicked="#showNextUser" text="Next" BorderPane.alignment="TOP_RIGHT" />
            </right>
        </BorderPane>
    </children>
</AnchorPane>
